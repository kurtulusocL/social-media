<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="kurtulusocL">
    <meta name="keyword" content="">
    <link rel="shortcut icon" href="~/img/Logo 3.png">
    <title>sOCiaL.com - @ViewBag.Title</title>

    <link href="~/Content/temp/Admin/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/PagedList.css" rel="stylesheet" />
    <link href="~/Content/temp/Admin/css/bootstrap-reset.css" rel="stylesheet">
    <link href="~/Content/temp/Admin/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css" />
    <link href="~/Content/temp/Admin/css/tasks.css" rel="stylesheet">
    <link href="~/Content/temp/Admin/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen" />
    <link rel="stylesheet" href="~/Content/temp/Admin/css/owl.carousel.css" type="text/css">
    <link rel="stylesheet" type="text/css" href="~/Content/temp/Admin/assets/gritter/css/jquery.gritter.css" />
    <link href="~/Content/temp/Admin/css/style.css" rel="stylesheet">
    <link href="~/Content/temp/Admin/css/style-responsive.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/Content/temp/Admin/assets/data-tables/DT_bootstrap.css" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <section id="container">
        <header class="header white-bg">
            <div class="sidebar-toggle-box">
                <div data-original-title="Toggle Navigation" data-placement="right" class="icon-reorder tooltips"></div>
            </div>
            <a href="~/Admin/Index" class="logo">sOCiaL<span>.com</span></a>
            @{
                Html.RenderAction("_Header", "Admin");
            }
        </header>
        <aside>
            @{
                Html.RenderAction("_Navbar", "Admin");
            }
        </aside>
        @RenderBody()

        <footer class="site-footer">
            @{
                Html.RenderAction("_Footer", "Admin");
            }
        </footer>
    </section>

    <script src="~/Content/temp/Admin/js/jquery.js"></script>
    <script src="~/Content/temp/Admin/js/jquery-1.8.3.min.js"></script>
    <script src="~/Content/temp/Admin/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="~/Content/temp/Admin/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Content/temp/Admin/js/jquery.scrollTo.min.js"></script>
    <script src="~/Content/temp/Admin/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="~/Content/temp/Admin/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="~/Content/temp/Admin/assets/jquery-knob/js/jquery.knob.js"></script>
    <script src="~/Content/temp/Admin/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="~/Content/temp/Admin/js/owl.carousel.js"></script>
    <script src="~/Content/temp/Admin/js/jquery.customSelect.min.js"></script>
    <script src="~/Content/temp/Admin/js/respond.min.js"></script>
    <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
    <script src="~/Content/temp/Admin/js/common-scripts.js"></script>
    <script src="~/Content/temp/Admin/js/sparkline-chart.js"></script>
    <script src="~/Content/temp/Admin/js/easy-pie-chart.js"></script>
    <script src="~/Content/temp/Admin/js/count.js"></script>
    <script type="text/javascript" src="~/Content/temp/Admin/assets/gritter/js/jquery.gritter.js"></script>
    <script src="~/Content/temp/Admin/js/gritter.js" type="text/javascript"></script>
    <script src="~/Content/temp/Admin/js/tasks.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Content/temp/Admin/assets/data-tables/jquery.dataTables.js"></script>
    <script type="text/javascript" src="~/Content/temp/Admin/assets/data-tables/DT_bootstrap.js"></script>
    <script src="~/Content/temp/Admin/js/editable-table.js"></script>

    <!-- END JAVASCRIPTS -->
    <script>
        jQuery(document).ready(function () {
            EditableTable.init();
        });
    </script>
    <script>
        jQuery(document).ready(function () {
            TaskList.initTaskWidget();
        });

        $(function () {
            $("#sortable").sortable();
            $("#sortable").disableSelection();
        });
    </script>
    <script>
        //owl carousel
        $(document).ready(function () {
            $("#owl-demo").owlCarousel({
                navigation: true,
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true,
                autoPlay: true
            });
        });
        //custom select box
        $(function () {
            $('select.styled').customSelect();
        });
    </script>
    <script>
        //knob
        $(".knob").knob();
    </script>
</body>
</html>

